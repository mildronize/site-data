{
  "name": "deventy_mildronize",
  "version": "1.2.3",
  "description": "An 11ty starter blog with modern tools",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist",
    "eleventy:default": "cross-env ELEVENTY_ENV=production node --max-old-space-size=4096 ./node_modules/@11ty/eleventy/cmd.js --quiet",
    "eleventy:watch": "cross-env ELEVENTY_ENV=development eleventy --watch",
    "eleventy:dev": "cross-env ELEVENTY_ENV=development eleventy",
    "serve": "light-server -c .lightserverrc",
    "dev": "npm-run-all clean eleventy:dev --parallel eleventy:watch serve --print-label",
    "dev:debug": "DEBUG=* npm run dev",
    "build": "run-s clean eleventy:default --print-label"
  },
  "repository": {
    "type": "git",
    "url": "git+https://ianrose@github.com/ianrose/deventy.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ianrose/deventy/issues"
  },
  "homepage": "https://github.com/ianrose/deventy#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.3",
    "light-server": "^2.6.3",
    "luxon": "^1.12.0",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.0.2",
    "markdown-it-attrs": "^3.0.1",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "acorn": "^6.1.1",
    "cross-env": "^6.0.3"
  }
}
